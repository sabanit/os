#include<iostream>
using namespace std;
int main()
{
    int n,c=0;
    cin>>n;
    int b[n],p[n];
    for(int i=0;i<n;i++)
    {
        cin>>b[i];
        c+=b[i];
        p[i]=c;
    }
    for(int i=0;i<n;i++)
        cout<<p[i]<<" ";
    cout<<endl;
    for(int i=0;i<n;i++)
        cout<<p[i]-b[i]<<" ";
}
